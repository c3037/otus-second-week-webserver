parameters:
  quit_command: 'quit'
  max_open_connections: 32
  max_waiting_connections: 128
  input_message_chunk_length: 1024
  host:
    cli_arguments:
      short: 'h'
      long: 'host'
    environment_variable: 'BRACKETS_SERVER_HOST'
    default: '0.0.0.0'
  port:
    cli_arguments:
      short: 'p'
      long: 'port'
    environment_variable: 'BRACKETS_SERVER_PORT'
    default: 19118
